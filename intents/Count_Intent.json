{
	"name": "Count_Intent",
        "description": "Recursively query the dataset",
        "conclusionStatement": {
          "messages": [
            {
              "content": "via code hook",
              "contentType": "PlainText"
            }
          ]
        },
        "fulfillmentActivity": {
          "codeHook": {
            "messageVersion": "1.0",
            "uri": "{{lambda-arn}}"
          },
          "type": "CodeHook"
        },
        "sampleUtterances": [
          "How much revenue was generated in {event_date}", 
          "How much money was generated in {event_date}", 
          "How many dollars were generated in {event_date}", 

          "Total revenue in {event_date}", 
          "Total money in {event_date}", 
          "Total dollars in {event_date}", 

          "Total revenue generated in {event_date}", 
          "Total money generated in {event_date}", 
          "Total dollars generated in {event_date}"
        ],
    "slots": [
        {
            "slotType": "AMAZON.DATE", 
            "name": "event_date", 
            "slotConstraint": "Required", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The year of the event", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "sampleUtterances": [], 
            "description": "event_year"
        }, 

        {
            "slotType": "AMAZON.Year", 
            "name": "event_year", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The year of the event", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "sampleUtterances": [], 
            "description": "event_year"
        }, 
        {
            "slotType": "AMAZON.Month", 
            "name": "event_month", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The month of the event", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "sampleUtterances": [], 
            "description": "event_month"
        }, 
        {
            "slotType": "AMAZON.Day", 
            "name": "event_day", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The day of the event", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "sampleUtterances": [], 
            "description": "event_day"
        } 
    ]
}
